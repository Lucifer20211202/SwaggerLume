{
  "name": "lumen-utils/swagger-lumen",
  "description": "OpenApi or Swagger integration to Lumen",
  "keywords": [
    "lumen",
    "swagger",
    "api",
    "OpenApi",
    "specification",
    "documentation",
    "API",
    "UI"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Lucifer",
      "email": "Lucifer20211202@protonmail.com"
    }
  ],
  "require": {
    "php": "~8.1",
    "laravel/lumen-framework": "^10.0",
    "zircote/swagger-php": "~4.7.15",
    "swagger-api/swagger-ui": "^4.0",
    "symfony/yaml": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "1.*",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "SwaggerLume\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "phpunit": "vendor/bin/phpunit"
  }
}
